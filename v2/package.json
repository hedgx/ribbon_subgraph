{
  "name": "v2",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create ribbon-finance/ribbon-finance-v2 --node https://api.thegraph.com/deploy/",
    "create-local": "graph create ribbon-finance/ribbon-finance-v2 --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "codegen-testnet": "graph codegen testnet.subgraph.yaml",
    "build-testnet": "graph build testnet.subgraph.yaml",
    "deploy": "graph deploy ribbon-finance/ribbon-finance-v2 --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy ribbon-finance/ribbon-finance-v2 --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy-testnet": "graph deploy ribbon-finance/ribbon-finance-v2-kovan --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "hardhat": "^2.6.1",
    "solc": "^0.8.7-fixed",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
